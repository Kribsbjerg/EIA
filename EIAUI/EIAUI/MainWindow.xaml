<Window x:Class="EIAUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EIAUI"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525"
        >
    <StackPanel>
        <TextBlock Text="Hello, World!" FontSize="40" FontFamily="{StaticResource LatoLight}"></TextBlock>
        <TextBlock Text="Hello, World!" FontSize="40" FontFamily="{StaticResource LatoRegular}"></TextBlock>
        <TextBlock Text="Hello, World!" FontSize="40" FontFamily="{StaticResource LatoBold}"></TextBlock>
        <TextBlock Text="Hello, World!" FontSize="40" FontFamily="{StaticResource LatoBlack}"></TextBlock>

        <Button Content="{Binding TestString}" Height="30" />

    </StackPanel>
    
</Window>